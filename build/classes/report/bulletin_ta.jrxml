	<queryString>
		<![CDATA[SELECT 
evaluation_classe.matricule,
evaluation_classe.classe,
evaluation_classe.annee,
evaluation_classe.semestre,
nom_prenom,
sexe,
rang,
(SELECT COUNT(matricule) FROM evaluation_classe WHERE classe = $P{classe} 
AND annee = $P{annee} AND semestre = $P{semestre}) AS effectif,

(SELECT moyenne FROM evaluation_classe WHERE classe = $P{classe} 
AND annee = $P{annee} AND semestre = 'S1' AND matricule = $P{matricule}) AS moyenne_s1,
(SELECT moyenne FROM evaluation_classe WHERE classe = $P{classe} 
AND annee = $P{annee} AND semestre = 'S2' AND matricule = $P{matricule}) AS moyenne_s2,
ROUND( ((SELECT moyenne FROM evaluation_classe WHERE classe = $P{classe} AND annee = $P{annee} AND semestre = 'S1' AND matricule = $P{matricule})+ ((SELECT moyenne FROM evaluation_classe WHERE classe = $P{classe} AND annee = $P{annee} AND semestre = 'S2' AND matricule = $P{matricule})))/2 ,2) AS moyenne_annuelle,

(SELECT SUM(coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE matiere.type = "LittÃ©raire" AND evaluation_matiere.classe = $P{classe} AND evaluation_matiere.annee = $P{annee} AND evaluation_matiere.matricule = $P{matricule} AND evaluation_matiere.semestre = $P{semestre}) AS coef_litteraire,

(SELECT SUM(note_coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE matiere.type = "LittÃ©raire" AND classe = $P{classe} AND annee = $P{annee} AND semestre = $P{semestre} AND matricule = $P{matricule}) AS note_litteraire,

ROUND((
(SELECT SUM(note_coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE matiere.type = "LittÃ©raire" AND classe = $P{classe} AND annee = $P{annee} AND semestre = $P{semestre} AND matricule = $P{matricule}) / 
(SELECT SUM(coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE matiere.type = "LittÃ©raire" AND matricule = $P{matricule} AND classe = $P{classe} AND annee = $P{annee})),2) AS moyen_litteraire,

(SELECT SUM(coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE matiere.type = "Scientifique" AND matricule = $P{matricule} AND classe = $P{classe} AND annee = $P{annee}) AS coef_scientifique,

(SELECT SUM(note_coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE matiere.type = "Scientifique" AND classe = $P{classe} AND annee = $P{annee} AND semestre = $P{semestre} AND matricule = $P{matricule}) AS note_scientifique,

ROUND((
(SELECT SUM(note_coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE matiere.type = "Scientifique" AND classe = $P{classe} AND annee = $P{annee} AND semestre = $P{semestre} AND matricule = $P{matricule}) / 
(SELECT SUM(coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE matiere.type = "Scientifique" AND matricule = $P{matricule} AND classe = $P{classe} AND annee = $P{annee})),2) AS moyen_scientifique,

(SELECT SUM(coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE matiere.type = "Autre" AND matricule = $P{matricule} AND classe = $P{classe} AND annee = $P{annee}) AS coef_autre,
(SELECT SUM(note_coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE matiere.type = "Autre" AND classe = $P{classe} AND annee = $P{annee} AND semestre = $P{semestre} AND matricule = $P{matricule}) AS note_autre,

ROUND((
(SELECT SUM(note_coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE matiere.type = "Autre" AND classe = $P{classe} AND annee = $P{annee} AND semestre = $P{semestre} AND matricule = $P{matricule}) / 
(SELECT SUM(coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE matiere.type = "Autre" AND matricule = $P{matricule} AND classe = $P{classe} AND annee = $P{annee})),2) AS moyen_autre,

(SELECT SUM(coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE classe = $P{classe} AND annee = $P{annee} AND matricule = $P{matricule}) AS coef_total,

(SELECT SUM(note_coefficient) FROM evaluation_matiere JOIN matiere ON evaluation_matiere.matiere = matiere.matiere WHERE classe = $P{classe} AND annee = $P{annee} AND semestre = $P{semestre} AND matricule = $P{matricule}) AS note_total,

moyenne,
moyenne_lettre,

(SELECT MAX(moyenne) FROM evaluation_classe WHERE classe = $P{classe} AND annee = $P{annee} AND semestre = $P{semestre} ) AS moyenne_max,

(SELECT MIN(moyenne) FROM evaluation_classe WHERE classe = $P{classe} AND annee = $P{annee} AND semestre = $P{semestre} ) AS moyenne_min,

ROUND( ((SELECT MAX(moyenne) FROM evaluation_classe WHERE classe = $P{classe} AND annee = $P{annee} AND semestre = $P{semestre} ) + (SELECT MIN(moyenne) FROM evaluation_classe WHERE classe = $P{classe} AND annee = $P{annee} AND semestre = $P{semestre} )) /2 ,2) AS moyenne_moy,
qr_code,
photo,

(case 
when moyenne between '0' AND '3.99' then '1'
else ''
end) AS t_blame,

(case 
when moyenne between '4' AND '6.99' then '1'
else ''
end) AS t_avertis,

(case 
when moyenne between '7' AND '9.99' then '1'
else ''
end) AS t_mauvaise,

(case 
when moyenne between '10' AND '11.99' then '1'
else ''
end) AS t_passable,
(case 
when moyenne between '12' AND '13.99' then '1'
else ''
end) AS t_assez_bien,
(case 
when moyenne between '14' AND '15.99' then '1'
else ''
end) AS t_bien,
(case 
when moyenne between '16' AND '17.99' then '1'
else ''
end) AS t_tres_bien,
(case 
when moyenne between '18' AND '20' then '1'
else ''
end) AS t_excellent,
(case 
when (SELECT moyenne FROM evaluation_matiere WHERE matiere = 'Conduite' AND annee = $P{annee} AND classe =  $P{classe} AND matricule = $P{matricule} AND semestre = $P{semestre}) between '18' AND '20' then '1'
else ''
end) AS c_tres_bien,
(case 
when (SELECT moyenne FROM evaluation_matiere WHERE matiere = 'Conduite' AND annee = $P{annee} AND classe =  $P{classe} AND matricule = $P{matricule} AND semestre = $P{semestre}) between '16' AND '17' then '1'
else ''
end) AS c_bien,
(case 
when (SELECT moyenne FROM evaluation_matiere WHERE matiere = 'Conduite' AND annee = $P{annee} AND classe =  $P{classe} AND matricule = $P{matricule} AND semestre = $P{semestre}) between '14' AND '15' then '1'
else ''
end) AS c_assez_bien,
(case 
when (SELECT moyenne FROM evaluation_matiere WHERE matiere = 'Conduite' AND annee = $P{annee} AND classe =  $P{classe} AND matricule = $P{matricule} AND semestre = $P{semestre}) between '10' AND '13' then '1'
else ''
end) AS c_passable,
(case 
when (SELECT moyenne FROM evaluation_matiere WHERE matiere = 'Conduite' AND annee = $P{annee} AND classe =  $P{classe} AND matricule = $P{matricule} AND semestre = $P{semestre}) between '8' AND '9' then '1'
else ''
end) AS c_mauvaise,
(case 
when (SELECT moyenne FROM evaluation_matiere WHERE matiere = 'Conduite' AND annee = $P{annee} AND classe =  $P{classe} AND matricule = $P{matricule} AND semestre = $P{semestre}) between '5' AND '7' then '1'
else ''
end) AS c_averstis,
(case 
when (SELECT moyenne FROM evaluation_matiere WHERE matiere = 'Conduite' AND annee = $P{annee} AND classe =  $P{classe} AND matricule = $P{matricule} AND semestre = $P{semestre}) between '0' AND '4' then '1'
else ''
end) AS c_blame,

(case 
when moyenne between '16' AND '20' then '1'
else ''
end) AS inscrit,
(case 
when moyenne between '14' AND '20' then '1'
else ''
end) AS felicitation,
(case 
when moyenne between '12' AND '13.99' then '1'
else ''
end) AS encouragement,
(case 
when moyenne between '0' AND '15.99' then '1'
else ''
end) AS non_inscrit

FROM evaluation_classe JOIN eleve ON 
evaluation_classe.matricule = eleve.matricule 
WHERE 
evaluation_classe.matricule =  $P{matricule} AND 
evaluation_classe.classe = $P{classe} AND 
evaluation_classe.semestre = $P{semestre} AND 
evaluation_classe.annee = $P{annee}]]>
	</queryString>
